<?xml version="1.0" encoding="UTF-8"?>

<suite name="LumaECommerceWebsiteAutomationSuite" parallel="tests" thread-count="3">
    <test name="ChromeTests">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="E_Commerce_Magento.LumaECommerceWebsiteAutomation" />
        </classes>
    </test>
    <test name="EdgeTests">
        <parameter name="browser" value="edge" />
        <classes>
            <class name="E_Commerce_Magento.LumaECommerceWebsiteAutomation" />
        </classes>
    </test>
    <test name="FirefoxTests">
        <parameter name="browser" value="firefox" />
        <classes>
            <class name="E_Commerce_Magento.LumaECommerceWebsiteAutomation" />
        </classes>
    </test>
</suite>
